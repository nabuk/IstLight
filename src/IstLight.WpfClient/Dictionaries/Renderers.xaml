<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:VM="clr-namespace:IstLight;assembly=IstLight.ViewModel"
                    xmlns:View="clr-namespace:IstLight">
    
    <DataTemplate DataType="{x:Type VM:TickerExplorerViewModel}">
        <View:TickerExplorerView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type VM:TickerProvidersViewModel}">
        <View:TickerProvidersView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type VM:ErrorListViewModel}">
        <View:ErrorListView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type VM:TickerFileViewModel}">
        <View:TickerFileView />
    </DataTemplate>
    
</ResourceDictionary>